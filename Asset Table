CREATE TABLE Assets(
Asset_ID BIGINT IDENTITY(1,1) NOT NULL,
Asset_No varchar(20) NOT NULL,
Asset_Description varchar(300)Not NULL,
Employee_ID BIGINT NOT NULL,
Asset_Category_ID BIGINT NOT NULL,
Status_ID BIGINT NOT NULL,
Department_ID BIGINT NOT NULL,
Vendor_ID BIGINT NOT NULL,
Make varchar(50) NOT NULL,
Model varchar(50) NOT NULL,
Model_Number varchar(50) NOT NULL,
Serial_Number varchar(50) NOT NULL,
Barcode_Number varchar(50) NOT NULL,
Date_Acquired datetime NOT NULL,
Date_Sold datetime NOT NULL,
Purchase_Price money NOT NULL,
Depreciation_Method varchar(50) NOT NULL,
Depreciable_Life INTEGER NOT NULL,
Salvage_Value MONEY NOT NULL,
Comments varchar NOT NULL,
Next_Sched_Maint datetime NOT NULL,
Transport_Install money NOT NULL, 
PRIMARY KEY (Asset_ID),
)
